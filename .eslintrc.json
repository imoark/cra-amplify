{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-console": "off",
    "react/prop-types": "off",
    "no-use-before-define": "off",
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": true
      }
    ]
  }
}
